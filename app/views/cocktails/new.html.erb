<h1>Create new - Cocktails</h1>
<div class="row justify-content-center">
  <div class="col-6">
    <%= simple_form_for(@cocktail) do |t| %>
      <%= t.input :name, input_html: {id:"banner-typed-text"}%>
      <%= t.input :photo, as: :file %>
      <%= t.submit%>
    <% end %>
    <%= link_to 'Back', cocktails_path, class: "btn btn-outline-secondary" %>
  </div>
</div>
